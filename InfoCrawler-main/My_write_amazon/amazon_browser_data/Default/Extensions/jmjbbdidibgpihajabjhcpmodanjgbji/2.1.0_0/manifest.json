      "default_title": "帮您淘"
   },
   "content_scripts": [ {
      "css": [ "css/injectScript.css" ],
      "exclude_matches": [ "*://*.gov.cn/*", "*://*.360.com/*", "*://*.360.cn/*", "*://*.so.com/*" ],
      "js": [ "js/request.js", "js/utils.js", "js/fp2.js", "js/contentScript.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
